<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/items_text_shop_goods_name"
        android:layout_width="189dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="0dp"
        android:layout_toStartOf="@+id/items_car_goods_number" />

    <TextView
        android:id="@+id/items_shop_goods_number"
        android:layout_width="161dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="7dp"
        android:layout_toStartOf="@+id/items_shop_goods_unit"
        android:layout_toEndOf="@+id/items_text_shop_goods_name" />

    <TextView
        android:id="@+id/items_shop_goods_unit"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="2dp" />
</RelativeLayout>
